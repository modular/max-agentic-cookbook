{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run copy:code:watch\" \"vite\"",
    "build": "npm run copy:code && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "copy:code": "node scripts/copy-recipe-code.js",
    "copy:code:watch": "node scripts/copy-recipe-code.js --watch"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.76",
    "@mantine/core": "^7.17.8",
    "@mantine/dropzone": "^7.17.8",
    "@mantine/hooks": "^7.17.8",
    "@mdx-js/rollup": "^3.1.1",
    "@tabler/icons-react": "^3.35.0",
    "ai": "^5.0.76",
    "nanoid": "^5.1.6",
    "pretty-ms": "^9.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4",
    "streamdown": "^1.4.0",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
